gcc -m32 -I lib/kernel/ -c -o kernel/main.o kernel/main.c && nasm -f elf -o lib/kernel/print.o lib/kernel/print.S && ld -m elf_i386 -Ttext 0xc0001500 -e main -o kernel.bin kernel/main.o lib/kernel/print.o && dd if=kernel.bin of=/home/zy/workspace/systemWorkspace/newSys/bochs/bochs/bin/hd60M.img bs=512 count=200 seek=9 conv=notrunc

